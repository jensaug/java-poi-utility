{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "_id": {
            "type": "string"
        },
        "measurement": {
            "type": "string"
        },
        "securityContext": {
            "type": "string"
        },
        "aspect": {
            "type": "string"
        },
        "dn": {
            "type": "string"
        },
        "filters": {
            "type": "string"
        },
        "status": {
            "type": "string"
        },
        "hsaId": {
            "type": "string"
        },
        "comment": {
            "type": "string"
        },
        "dimensions": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "percentagesPositive": {
            "type": "array",
            "items": {
                "type": "number"
            }
        },
        "trends": {
            "type": "array",
            "items": {
                "type": "number"
            }
        },
        "invitees": {
            "type": "object",
            "properties": {
                "answered": {
                    "type": "number"
                },
                "invited": {
                    "type": "number"
                },
                "invitedAdjusted": {
                    "type": "number"
                },
                "answeredPercentage": {
                    "type": "number"
                },
                "answeredAdjustedPercentage": {
                    "type": "number"
                }
            },
            "required": [
                "answered",
                "invited",
                "invitedAdjusted",
                "answeredPercentage",
                "answeredAdjustedPercentage"
            ],
            "additionalProperties": false
        }
    },
    "required": [
        "measurement",
        "aspect",
        "dn",
        "status",
        "dimensions",
        "invitees"
    ],
    "oneOf": [
        {
            "required": ["comment"]
        },
        {
            "required": ["percentagesPositive"]
        }
    ],
    "additionalProperties": false
}